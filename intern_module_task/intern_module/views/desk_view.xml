<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    
    <record id="desk_list_view_form" model="ir.ui.view">
        <field name="name">List View</field>
        <field name="model">desk.allocation</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="desk_no"/>
            </tree>
        </field>
    </record>


    <record id="desk_list_form" model="ir.ui.view">
        <field name="name">List View</field>
        <field name="model">desk.allocation</field>
        <field name="arch" type="xml">
           <form>
            <sheet>
                <h1>
                    <field name="name" />
                </h1>
                <notebook>
                    <page name="intern">
                        <field name="desk_no_ids" type="xml">
                            <tree>
                                <field name="name"></field>
                                <field name="intern_type"></field>
                                <field name="email_id"></field>
                            </tree>
                        </field>
                    </page>
                </notebook>
            </sheet>
           </form>
        </field>
    </record>
        
</odoo>
        
